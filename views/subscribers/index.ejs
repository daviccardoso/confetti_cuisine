<div class="list-container">
  <% subscribers.forEach(s => { %>
  <div class="subscriber">
    <p>
      <span class="item-label">Name:</span>
      <a href="<%= `/subscribers/${s._id}` %>"><%= s.name %></a>
    </p>
    <p>
      <span class="item-label">Email:</span>
      <%= s.email %>
    </p>
    <p>
      <span class="item-label"><a href="<%= `/subscribers/${s._id}/edit` %>">Edit</a></span>
    </p>
    <p>
      <span class="item-label"><a href="<%= `/subscribers/${s._id}/delete?_method=DELETE` %>"
          onclick="return confirm('Are you shure you want to delete this record?')">Delete</a></span>
    </p>
  </div>
  <% }); %>
</div>
